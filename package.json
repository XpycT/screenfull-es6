{
  "name": "screenfull-es6",
  "version": "0.1.1",
  "description": "Simple wrapper for cross-browser usage of the JavaScript Fullscreen API",
  "main": "dist/screenfull-es6.js",
  "module": "dist/screenfull-es6.esm.js",
  "jsnext:main": "dist/screenfull-es6.esm.js",
  "unpkg": "dist/screenfull-es6.min.js",
  "iife": "dist/screenfull-es6.browser.js",
  "iifemin": "dist/screenfull-es6.browser.min.js",
  "scripts": {
