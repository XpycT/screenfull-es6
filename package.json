    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "shx": "^0.3.2",
    "gh-pages": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/XpycT/screenfull-es6.git"
